<nav class="">
    <ul>
        <%if !user_signed_in?%>
          <l1><%=link_to "Sign up", new_user_registration_path%></l1>
          <l1><%=link_to "sign in", new_user_session_path%></l1>
        <%else%>
          <l1><%=link_to "Signed in as #{current_user.email}", edit_user_registration_path%></l1>
          <l1><%=link_to "Sign out", destroy_user_session_path, method: :delete%></l1>
        <%end%>

        <li> <%= link_to "Store Home", store_path %> </li>
        <% if current_user != nil %>
          <% if current_user.current_cart != nil%>
            <li> <%= link_to "Cart", cart_path(current_user.current_cart) %></li>
          <% end %>
        <%end%>
    </ul>
</nav>
